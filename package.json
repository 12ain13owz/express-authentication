  "main": "main.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "prod:migrate": "cross-env NODE_ENV=production dotenv -e .env.production -- npx prisma migrate deploy",
    "dev": "cross-env NODE_ENV=development tsx watch src/main",
    "dev:migrate": "cross-env NODE_ENV=development dotenv -e .env.development -- npx prisma migrate dev",
    "dev:studio": "cross-env NODE_ENV=development dotenv -e .env.development -- npx prisma studio",
    "dev:reset": "cross-env NODE_ENV=development dotenv -e .env.development -- npx prisma migrate reset",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc && tsc-alias && cp -r src/templates dist/",
    "setup-env": "node scripts/setup-env.mjs",
    "prisma:generate": "cross-env dotenv -- npx prisma generate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
